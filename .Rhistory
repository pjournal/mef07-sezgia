# Load ggplot2 library
library(ggplot2)
# Sample data
cars <- mtcars
cars$name <- rownames(cars)
# Generate random data
set.seed(123)
data <- data.frame(
x = rnorm(10),
y = rnorm(10),
size = runif(10, min = 5, max = 20)
)
# Create a bubble chart
ggplot(data, aes(x, y, size = size)) +
geom_point() +
scale_size_continuous(range = c(3, 10)) +
labs(
title = "Basic Bubble Chart",
x = "X-Axis",
y = "Y-Axis",
size = "Y") +
theme_minimal()
